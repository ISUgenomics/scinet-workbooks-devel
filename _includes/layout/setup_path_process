    {%- assign setup = page.setup %}
    {%- if page.mkdir %}
        {% assign envtitle = page.mkdir %}
    {%- else %}
        {% include components/file_process folder=include.folder url=setup %}
        {% assign rev_path = module_sections | reverse %}
        {% assign envtitle = rev_path.first %}
    {%- endif %}
{%- else %}
