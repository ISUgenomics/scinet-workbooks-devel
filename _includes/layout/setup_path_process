    {%- assign setup = module_sections | join: "/" %}

    {%- if page.mkdir %}
        {% assign envtitle = page.mkdir %}
    {%- else %}
        {% assign rev_path = module_sections | reverse %}
        {% assign envtitle = rev_path.first %}
    {%- endif %}
{%- endif %}